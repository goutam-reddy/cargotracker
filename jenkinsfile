pipeline {
      agent any
      stages  {
               stage('One') {
               steps {
                   echo 'hi, this is hariprasad'
               }
               }
               stage('Two') {
               steps {
                   input('Do you want to proceed')
               }
               }
               stage('three') {
               when {
                     not {
                          branch "master"
                     }
                }
                steps {
                      echo "Hello"
                }
                }
                stage('Unit Test') {
                          steps {
                             echo "Running the unit test..."
                          }
                          }
                   }
            }
